{
	"name": "my-home-app",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"css-build-light": "node-sass --omit-source-map-url sass/lightstyle.scss static/lightstyle.css",
		"css-build-dark": "node-sass --omit-source-map-url sass/darkstyle.scss static/darkstyle.css",
		"css-build": "npm run css-build-light && npm run css-build-dark",
		"css-watch": "npm run css-build -- --watch",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"test:unit": "vitest",
		"lint": "prettier --plugin-search-dir . --check . && eslint .",
		"format": "prettier --plugin-search-dir . --write ."
	},
	"devDependencies": {
		"@sveltejs/adapter-auto": "^2.1.0",
		"@sveltejs/adapter-static": "^2.0.2",
		"@sveltejs/kit": "^1.22.3",
		"@typescript-eslint/eslint-plugin": "^6.1.0",
		"@typescript-eslint/parser": "^6.1.0",
		"bulma": "^0.9.4",
		"csvtojson": "^2.0.10",
		"dayjs": "^1.11.9",
		"eslint": "^8.45.0",
		"eslint-config-prettier": "^8.8.0",
		"eslint-plugin-svelte": "^2.32.2",
		"highlight.js": "^11.8.0",
		"node-sass": "^9.0.0",
		"prettier": "^3.0.0",
		"prettier-plugin-svelte": "^3.0.0",
		"sass": "^1.66.1",
		"svelte": "^4.0.5",
		"svelte-check": "^3.4.6",
		"svelte-facebook-login": "^2.0.0-rc2",
		"svelte-preprocess": "^5.0.4",
		"tslib": "^2.6.0",
		"typescript": "^5.1.6",
		"typescript-svelte-plugin": "^0.3.29",
		"vite": "^4.4.4",
		"vitest": "^0.33.0",
		"wretch": "^2.7.0"
	},
	"type": "module"
}
