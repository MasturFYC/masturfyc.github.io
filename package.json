{
  "name": "svelte-app",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "rollup -c",
    "dev": "rollup -c -w",
    "start": "sirv docs --no-clear --single",
    "watch": "rollup -c rollup.config.js --watch",
    "prepare": "npm run smui-theme-light && npm run smui-theme-dark",
    "smui-theme-light": "smui-theme compile docs/smui.css -i src/theme",
    "smui-theme-dark": "smui-theme compile docs/smui-dark.css -i src/theme/dark"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^24.1.0",
    "@rollup/plugin-node-resolve": "^15.1.0",
    "@rollup/plugin-replace": "^5.0.2",
    "@rollup/plugin-terser": "^0.4.0",
    "@rollup/plugin-typescript": "^11.1.2",
    "@smui-extra/autocomplete": "^7.0.0-beta.14",
    "@smui/button": "^7.0.0-beta.14",
    "@smui/dialog": "^7.0.0-beta.14",
    "@smui/form-field": "^7.0.0-beta.14",
    "@smui/icon-button": "^7.0.0-beta.14",
    "@smui/radio": "^7.0.0-beta.14",
    "@smui/select": "^7.0.0-beta.14",
    "@smui/switch": "^7.0.0-beta.14",
    "@smui/textfield": "^7.0.0-beta.14",
    "@smui/top-app-bar": "^7.0.0-beta.14",
    "@sveltestack/svelte-query": "^1.6.0",
    "@tsconfig/svelte": "^5.0.0",
    "axios": "^1.4.0",
    "dayjs": "^1.11.9",
    "dotenv": "^16.3.1",
    "rollup": "^3.26.3",
    "rollup-plugin-css-only": "^4.3.0",
    "rollup-plugin-livereload": "^2.0.0",
    "rollup-plugin-postcss": "^4.0.2",
    "rollup-plugin-svelte": "^7.1.6",
    "sass": "^1.64.1",
    "smui-theme": "^7.0.0-beta.14",
    "svelte": "^4.1.1",
    "svelte-preprocess": "^5.0.4",
    "tslib": "^2.6.0",
    "typescript": "^5.1.6"
  },
  "dependencies": {
    "sirv-cli": "^2.0.0",
    "svelte-spa-router": "^3.3.0"
  }
}
